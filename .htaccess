RewriteEngine on

RewriteCond %{HTTP_REFERER} !^http://jonescontractinginc.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://jonescontractinginc.com$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.jonescontractinginc.com/.*$      [NC]
RewriteCond %{HTTP_REFERER} !^http://www.jonescontractinginc.com$      [NC]
RewriteRule .*\.(jpg|jpeg|gif|png|bmp|pdf)$ - [F,NC]

Options -MultiViews

<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /php/catch_path.php [L,NC,QSA]
</IfModule>